conda install -c conda-forge jupyter_contrib_nbextensions
jupyter contrib nbextension install --user


git clone https://github.com/gesiscss/widd srv/conda/envs/notebook/share/jupyter/nbextensions/widd
jupyter nbextension install srv/conda/envs/notebook/share/jupyter/nbextensions/widd --user
jupyter nbextension enable widd/main

